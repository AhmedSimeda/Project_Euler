   ],
   "source": [
    "count = 0\n",
    "for n in range(2,101):\n",
    "    for r in range(n-1,0,-1):\n",
    "        if nCr(n,r) > 1000000:\n",
    "            count +=1\n",
    "\n",
    "count"
   ]
