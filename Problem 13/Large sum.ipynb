    "def get_sol(fileName):\n",
    "    Sum = 0\n",
    "    with open(fileName) as numbers:\n",
    "        for number in numbers:\n",
    "            Sum += int(number.strip())\n",
    "    \n",
    "    print(\"The First ten digits are :\" + str(Sum)[:10])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
